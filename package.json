{
  "name": "my-next-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "22.12.0"
  },
  "scripts": {
    "backend:nodemon": "nodemon --watch ./backend --exec 'json-server ./backend/db.json --port 3001 --middlewares ./backend/server.middleware.js'",
    "backend": "json-server ./backend/db.json --port 3001 --middlewares ./backend/server.middleware.js",
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "build:webpack": "NEXTJS_BUNDLE_ANALYZE=true next build",
    "start": "next start",
    "start:manually": "npx http-server ./.next/server/app",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:check": "eslint --inspect-config"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.9.1",
    "next": "^16.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-redux": "^9.2.0",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^16.0.1",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/styled-components": "^5.1.34",
    "eslint": "^9",
    "eslint-config-next": "^16.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "prettier": "3.6.2",
    "typescript": "^5",
    "typescript-eslint": "^8.46.2"
  }
}
